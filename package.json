{
  "name": "drupal-module-init",
  "version": "1.0.2",
  "description": "Import starter template repository files into a drupal module/theme",
  "main": "index.mjs",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/raphaelApard/init-drupal-module.git"
  },
  "scripts": {
    "build": "vite build --mode development",
    "watch": "vite build --watch --mode development",
    "test": "jest --passWithNoTests",
    "lint": "eslint --ext .js,.ts ./ts/**/*.{js,ts}",
    "ci:build": "yarn run build --mode production --logLevel error"
  },
  "bin": "./index.mjs",
  "dependencies": {
    "@inquirer/prompts": "^2.1.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "glob": "^10.2.7",
    "chalk": "^5.2.0",
    "uniqid": "^5.4.0",
    "yaml": "^2.3.1"
  }
}
